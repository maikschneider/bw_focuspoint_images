{
  "name": "blueways/bw-focuspoint-images",
  "type": "typo3-cms-extension",
  "description": "Image Editor for adding focus points to images",
  "homepage": "https://blueways.de",
  "license": [
    "GPL-2.0-or-later"
  ],
  "keywords": [
    "TYPO3 CMS"
  ],
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "typo3/cms-base-distribution": "^11.0",
    "bk2k/bootstrap-package": "dev-master",
    "helhum/typo3-console": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "Blueways\\BwFocuspointImages\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "web-dir": "public",
      "extension-key": "bw_focuspoint_images"
    }
  },
  "authors": [
    {
      "homepage": "https://blueways.de",
      "name": "Maik Schneider",
      "role": "Developer",
      "email": "m.schneider@blueways.de"
    }
  ],
  "require": {
    "ext-json": "*",
    "typo3/cms-core": "^9.0 || ^10.0 || ^11.0"
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  }
}
